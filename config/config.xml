<?xml version="1.0" encoding="UTF-8"?>
<config>
    <paths>
        <plots_dir>plots</plots_dir>
        <agents_dir>agents</agents_dir>
        <environments_dir>environments</environments_dir>
    </paths>
    <simulation>
        <n_steps>300</n_steps>
        <n_trials>2</n_trials>
        <confidence_levels>
            <level>0.95</level>
        </confidence_levels>
    </simulation>
    <seeds>
        <numpy>42</numpy>
        <random>42</random>
    </seeds>
    <environments>
        <n_actions>10</n_actions>
        <bernoulli>
            <probs>
                <prob>0.1</prob>
                <prob>0.2</prob>
                <prob>0.3</prob>
                <prob>0.4</prob>
                <prob>0.5</prob>
                <prob>0.6</prob>
                <prob>0.7</prob>
                <prob>0.8</prob>
                <prob>0.9</prob>
                <prob>1.0</prob>
            </probs>
        </bernoulli>
        <gaussian>
            <means>
                <mean>0.1</mean>
                <mean>0.2</mean>
                <mean>0.3</mean>
                <mean>0.4</mean>
                <mean>0.5</mean>
                <mean>0.6</mean>
                <mean>0.7</mean>
                <mean>0.8</mean>
                <mean>0.9</mean>
                <mean>1.0</mean>
            </means>
            <stds>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
                <std>0.1</std>
            </stds>
        </gaussian>
    </environments>
</config> 
